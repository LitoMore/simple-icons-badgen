    "simple-icons": "^7.13.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.23",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
